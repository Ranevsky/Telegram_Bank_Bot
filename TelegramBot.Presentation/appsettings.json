{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.EntityFrameworkCore": "Error"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TelegramConnection": "",
    "BankConnection": ""
  },
  "Telegram": {
    "Token": ""
  },
  "Google": {
    "Key": "",
    "Language": ""
  },
  "BankApi": {
    "UpdateTimeInSecond": 3600
  }
}